<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox stylesheets="@Styles.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.triviamaze.MainMenuController">
   <children>
      <Pane fx:id="MainBackground" prefHeight="730.0" prefWidth="1300.0">
         <children>
            <Button fx:id="Scoreboard" layoutX="460.0" layoutY="500.0" mnemonicParsing="false" onAction="#scoreboardButtonClicked" prefHeight="118.0" prefWidth="380.0" />
            <Button fx:id="NewGame" layoutX="460.0" layoutY="372.0" mnemonicParsing="false" onAction="#startGame" prefHeight="118.0" prefWidth="380.0" />
            <Button fx:id="PreviousGame" layoutX="460.0" layoutY="244.0" mnemonicParsing="false" onAction="#loadGame" prefHeight="118.0" prefWidth="380.0" />
            <Button fx:id="Exit" layoutX="1180.0" layoutY="580.0" mnemonicParsing="false" onAction="#exitButtonClicked" prefHeight="90.0" prefWidth="90.0" />
            <Button fx:id="Settings" layoutX="1180.0" layoutY="480.0" mnemonicParsing="false" onAction="#settingsButtonClicked" prefHeight="90.0" prefWidth="90.0" />
            <Button fx:id="helpButton" layoutX="1183.0" layoutY="381.0" mnemonicParsing="false" onAction="#helpButtonClicked" prefHeight="85.0" prefWidth="85.0" />

         </children></Pane>
   </children>
</VBox>
